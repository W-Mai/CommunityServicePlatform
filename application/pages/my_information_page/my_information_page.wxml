<view class="top-block">
  <view class="one-row">
    <view class="one-row-key">头像</view>
    <view>
      <view class="one-row-value">
        <view>
          <image class="profile"
            src="/images/ZZULI Logo Blue.png"
            mode="aspectFit"></image>
        </view>
        <view class="one-row-more-button">›</view>
      </view>
    </view>
  </view>

  <navigator url="/pages/editior_page/editior_page?info=姓名&data={{userInfo['姓名']}}">
    <view class="one-row">
      <view class="one-row-key">姓名</view>
      <view>
        <view class="one-row-value">
          <view>{{userInfo["姓名"]}}</view>
          <view class="one-row-more-button">›</view>
        </view>
      </view>
    </view>
  </navigator>

  <picker bindchange="bindPickerChange" value="{{gender_index}}" range="{{gender_array}}" data-id="gender">
    <view class="one-row">
      <view class="one-row-key">性别</view>
      <view>
        <view class="one-row-value">
          {{gender_array[gender_index]}}
          <view class="one-row-more-button">›</view>
        </view>
        <!-- <view class="one-row-value"><view>{{name}}</view><view class="one-row-more-button">›</view></view> -->
      </view>
    </view>
  </picker>

  <picker mode="date" bindchange="bindPickerChange" value="{{currentDate}}" data-id="birthday" start="1995-01-01" end="2099-12-31">
    <view class="one-row">
      <view class="one-row-key">出生日期</view>
      <view>
        <view class="one-row-value">
          {{currentDate}}
          <view class="one-row-more-button">›</view>
        </view>
        <!-- <view class="one-row-value"><view>{{name}}</view><view class="one-row-more-button">›</view></view> -->
